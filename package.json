{
  "name": "vite-plugin-unique-page-chunks",
  "version": "1.0.1",
  "description": "A Vite plugin to generate unique chunks for pages with same-named components",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "types": "./index.d.ts",
  "files": [
    "dist",
    "README.md",
    "LICENSE",
    "index.d.ts"
  ],
  "scripts": {
    "build": "rollup -c",
    "prepublishOnly": "npm run build",
    "test": "vitest run",
    "test:watch": "vitest"
  },
  "keywords": [
    "vite",
    "vite-plugin",
    "chunks",
    "unique",
    "page"
  ],
  "author": "fecym",
  "license": "ISC",
  "peerDependencies": {
    "vite": ">=2.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.28.4",
    "@babel/preset-env": "^7.28.3",
    "@rollup/plugin-babel": "^6.0.4",
    "@rollup/plugin-commonjs": "^25.0.0",
    "@rollup/plugin-node-resolve": "^15.0.0",
    "@rollup/plugin-terser": "^0.4.4",
    "@vitest/ui": "^3.2.4",
    "rollup": "^3.0.0",
    "vite": "^7.1.6",
    "vitest": "^3.2.4"
  },
  "engines": {
    "node": ">=14"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/fecym/vite-plugin-unique-page-chunks.git"
  },
  "bugs": {
    "url": "https://github.com/fecym/vite-plugin-unique-page-chunks/issues"
  },
  "homepage": "https://github.com/fecym/vite-plugin-unique-page-chunks#readme"
}
